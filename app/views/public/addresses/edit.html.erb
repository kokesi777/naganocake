<h3>配送先編集</h3>
<div>
    <%= form_for(@address, url: address_path(@address.id)) do |f| %>
        <label>郵便番号</label>
        <%= f.text_field :post_code %> </br>
        <label>住所</label>
        <%= f.text_field :address %><br/>
        <label>宛名</label>
        <%= f.text_field :name %></br>
        

        <%= f.submit '編集する', class: "btn btn-success" %>
    <% end %>
</div>


