<div>  
    <h2>会員一覧</h2>
</div>

<div>
    <table class="table">
        <thead class="active">
            <tr>
                <th>会員ID</th>
                <th>氏名</th>
                <th>メールアドレス</th>
                <th>ステータス</th>
            </tr>
        </thead>
        <tbody>
            <% @end_users.each do |end_users| %>
                <tr>
                <td><%= end_users.id %></td>
                <td><%= end_users.last_name %><%= end_users.first_name %></td>
                <td><%= end_users.email %></td>
                <td><%= end_users.is_delete %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
</div>





